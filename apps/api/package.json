{
  "name": "@workspace/api",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js",
    "watch-ts": "concurrently -k -p \"[{name}]\" -n \"tsc,tsc-alias\" -c \"cyan.bold,green.bold\" \"tsc -w\" \"tsc-alias -w\"",
    "watch-node": "nodemon dist/index.js",
    "perdev": "rimraf dist && tsc",
    "dev:old": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"pnpm watch-ts\" \"pnpm watch-node\"",
    "dev:new": "tsup --watch --onSuccess \"node dist/index.js\"",
    "dev": "nodemon",
    "build": "tsup",
    "build:old": "tsc && tsc-alias",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write ."
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "axios": "^1.7.9",
    "bullmq": "^5.35.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4",
    "morgan": "^1.10.0",
    "pretty-ms": "^9.2.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.10.10",
    "@types/node-fetch": "^2.6.12",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "@workspace/auth": "workspace:*",
    "@workspace/database": "workspace:*",
    "@workspace/env": "workspace:*",
    "@workspace/logger": "workspace:^",
    "@workspace/trpc": "workspace:*",
    "@workspace/typescript-config": "workspace:*",
    "concurrently": "^9.1.2",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "nodemon": "^3.1.9",
    "prettier": "^3.2.5",
    "rimraf": "^6.0.1",
    "tsc-alias": "^1.8.10",
    "tsup": "^8.3.6",
    "typescript": "^5.6.3"
  }
}
