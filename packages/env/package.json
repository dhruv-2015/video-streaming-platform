{
  "name": "@workspace/env",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "package.json"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./next": {
      "types": "./dist/nextjs.d.ts",
      "import": "./dist/nextjs.mjs",
      "require": "./dist/nextjs.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "check": "tsc --noEmit",
    "predev": "tsup",
    "dev": "tsup --watch"
  },
  "dependencies": {
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "22.9.0",
    "tsup": "^8.3.6",
    "@workspace/typescript-config": "workspace:*",
    "typescript": "^5.6.3",
    "valibot": "1.0.0-beta.14"
  }
}
